{
  "name": "alto-2",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "pnpm tsx ./clone.ts && remix vite:build",
    "dev": "pnpm tsx ./clone.ts && remix vite:dev",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "remix-serve ./build/server/index.js",
    "db:memory:generate": "rm -rf drizzle.memory && pnpm drizzle-kit generate:sqlite --schema=./app/services/database/schema.ts --out=drizzle.memory",
    "db:test:push": "rm -rf drizzle.test && pnpm drizzle-kit push --config drizzle.test.config.ts",
    "db:test:reset": "rm test.sqlite && pnpm db:test:push",
    "db:test:studio": "pnpm drizzle-kit studio --config drizzle.test.config.ts",
    "db:turso:push": "rm -rf drizzle.turso && pnpm drizzle-kit push --config drizzle.turso.config.ts",
    "db:turso:generate": "pnpm drizzle-kit generate:sqlite --config drizzle.turso.config.ts",
    "db:turso:reset": "rm turso.sqlite && pnpm db:turso:push",
    "db:turso:studio": "pnpm drizzle-kit studio --config drizzle.turso.config.ts",
    "typecheck": "tsc",
    "tsx": "tsx"
  },
  "dependencies": {
    "@libsql/client": "0.5.6",
    "@remix-run/node": "^2.8.1",
    "@remix-run/react": "^2.8.1",
    "@remix-run/serve": "^2.8.1",
    "@types/diff3": "^0.0.2",
    "@vercel/remix": "^2.9.1",
    "async-lock": "^1.1.0",
    "better-sqlite3": "^9.5.0",
    "clean-git-ref": "^2.0.1",
    "clsx": "^2.1.1",
    "crc-32": "^1.2.0",
    "diff3": "0.0.3",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.30.8",
    "ignore": "^5.1.4",
    "isbot": "^4.1.0",
    "isomorphic-git": "^1.25.7",
    "lucide-react": "^0.377.0",
    "minimisted": "^2.0.0",
    "node-git-server": "^1.0.0",
    "pako": "^1.0.11",
    "pify": "^4.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "readable-stream": "^3.4.0",
    "sha.js": "^2.4.9",
    "simple-get": "^4.0.1",
    "zod": "^3.23.4"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.8.1",
    "@types/better-sqlite3": "^7.6.9",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "autoprefixer": "^10.4.19",
    "drizzle-kit": "^0.21.1",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "tmp-promise": "^3.0.3",
    "tsx": "4.9.0",
    "typescript": "^5.1.6",
    "vite": "^5.1.0",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^1.5.0"
  },
  "engines": {
    "node": "20"
  }
}
